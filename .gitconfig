[alias]
    st = status
    br = branch
    co = checkout
    last = log -1 HEAD
    unstage = reset HEAD --
    tree = log --pretty=format:\"%h %s\" --graph
    trees = log --all --decorate --oneline --graph

[pull]
    ff = only

[user]
    name = rasca0027

[core]
    pager = delta

[interactive]
    diffFilter = delta --color-only

[delta]
    navigate = true    # use n and N to move between diff sections
    light = false      # set to true if you're in a terminal w/ a light background color (e.g. the default macOS terminal)
    side-by-side = true
    line-numbers = true

[merge]
    conflictstyle = diff3

[diff]
    colorMoved = default
[hub]
    protocol = ssh
